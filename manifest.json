{
    "manifest_version": 2,

    "name": "prettifyCSCI",
    "description": "Prettify the CSCI assignment page from Dr. Rader. Refresh the page to reset.",
    "version": "0.1",

    "browser_action": {
        "default_title": "prettifyCSCI",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ],

    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"

}
